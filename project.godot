; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Gamba"
run/main_scene="res://Scenes/main.tscn"
config/features=PackedStringArray("4.0", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Score="*res://MISC/singletons/Score.gd"
Server="*res://MISC/singletons/Server.gd"
Data="*res://MISC/singletons/Data.gd"
SwipeDetection="res://MISC/singletons/SwipeDetection.gd"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="ignore"
window/vsync/vsync_mode=0

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/physics_ticks_per_second=120

[rendering]

anti_aliasing/screen_space_roughness_limiter/amount=0.72
anti_aliasing/screen_space_roughness_limiter/limit=0.31
anti_aliasing/quality/msaa_2d=1
anti_aliasing/quality/msaa_3d=1
anti_aliasing/quality/use_debanding=true
